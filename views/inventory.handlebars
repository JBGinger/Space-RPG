<div class="main-container">
    {{!-- <div id="welcome-text"></div> --}}
    {{!-- {{>inventory-table}} --}}

    <div id="user-inventory">
        {{user.username}}'s inventory
    </div>

    <table>
        <tr>
            <th> Item </th>
            <th> Rarity </th>
            <th> Price </th>
            {{!-- <th> Planet </th> --}}
        </tr>
        {{!-- {{#each this}} --}}
        <tr>
            <td> {{item.item_name}} </td>
            <td> {{item.rarity}} </td>
            <td> {{item.price}} </td>
            {{!-- <td> Mars </td> --}}
        </tr>
        {{!-- {{/each}} --}}
    </table>
    

</div>


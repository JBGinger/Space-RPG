<div id="user-inventory">
    {{name}}'s inventory
</div>

<table>
    <tr>
        <th> Items </th>
        <th> Rarity </th>
        <th> Price </th>
        {{!-- <th> Planet </th> --}}
    </tr>
    {{#each this}}
    <tr>
        <td> {{itemData.item_name}} </td>
        <td> {{itemData.rarity}} </td>
        <td> {{itemData.price}} </td>
        {{!-- <td> Mars </td> --}}
    </tr>
    {{/each}}
</table>